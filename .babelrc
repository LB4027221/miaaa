{
  "plugins": [
    ["@babel/plugin-syntax-decorators", { "legacy": true }],
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    ["import", { "libraryName": "antd", "style": false }]
  ],
  "presets": [
    "@babel/preset-flow",
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": "commonjs",
      "targets": {
        "esmodules": true
      }
    }]
  ]
}
